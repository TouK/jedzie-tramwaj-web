{
  "name": "tramap",
  "version": "0.0.1",
  "description": "warsaw tram map with real gps positions",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run bower",
    "bower": "bower prune && bower install",
    "prebower": "npm run prepare",
    "prepare": "coffee scripts/prepare.coffee",
    "clean": "coffee scripts/clean.coffee",
    "build": "brunch build --production",
    "start": "brunch watch --server",
    "pretest": "npm install",
    "test": "karma start test/karma.conf.coffee",
    "test-single-run": "karma start test/karma.conf.coffee --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor test/protractor.conf.coffee"
  },
  "author": "Julian Wielga",
  "license": "ISC",
  "devDependencies": {
    "auto-reload-brunch": ">= 1.0 < 2.0",
    "bower": ">= 1.4 < 1.5",
    "brunch": ">= 1.0 < 2.0",
    "clean-css-brunch": ">= 1.0 < 2.0",
    "coffee-script": ">= 1.9 < 2.0",
    "coffee-script-brunch": ">= 1.8 < 2.0",
    "javascript-brunch": ">= 1.0 < 2.0",
    "constangular-brunch": "^0.1.4",
    "css-brunch": ">= 1.0 < 2.0",
    "less-brunch": ">= 1.0 < 2.0",
    "stylus-brunch": ">= 1.0 < 2.0",
    "uglify-js-brunch": ">= 1.0 < 2.0",
    "stylus-brunch": "^1.8.1",
    "uglify-js-brunch": "^1.7.8",
    "jade": ">= 1.9 < 2.0",
    "jade-ngtemplates-brunch": ">= 1.7.0 < 1.8.0",
    "jade-pages-brunch": ">= 1.7.0 < 1.8.0",
    "jasmine-core": "^2.2.0",
    "cordova": "^5.0.0",
    "karma": ">= 0.12.0 < 0.13.0",
    "karma-chrome-launcher": "~0.1.4",
    "karma-cli": "0.0.4",
    "karma-coffee-preprocessor": ">= 0.2.0",
    "karma-coverage": "~0.2.6",
    "karma-html-reporter": "~0.2.3",
    "karma-jasmine": ">= 0.2.0 < 0.3.0",
    "karma-junit-reporter": "~0.2.2",
    "karma-ng-classify-preprocessor": "^1.0.0",
    "karma-phantomjs-launcher": "~0.1.4",
    "npm": ">= 2.7 < 2.8",
    "protractor": ">= 1.0 < 1.1",
    "shelljs": ">= 0.4",
    "require-yaml": "0.0.1"
  }
}
